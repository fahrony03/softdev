        android:supportsRtl="true"
        android:theme="@style/Theme.SIPEKA">
        <activity android:name=".DetailProduk"></activity>
        <activity android:name=".DetailArtikel"></activity>
        <activity android:name=".DetailKategori1"></activity>
        <activity android:name=".DetailKategori2"></activity>
        <activity android:name=".DetailKategori3"></activity>
        <activity android:name=".DetailKategori4"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>